{
  "name": "@dboard/frontend",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --host",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write 'src/**/*.+(vue|js|jsx|cjs|mjs|ts|tsx|cts|mts)'",
    "precommit": "npm run format && npm run lint",
    "json-server": "json-server --watch data/db.json --port 3001"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-regular-svg-icons": "^6.0.0-beta3",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/vue-fontawesome": "github:FortAwesome/vue-fontawesome#3.x",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.12",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-plugin-vue": "^8.4.0",
    "json-server": "^0.17.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.31.1"
  }
}